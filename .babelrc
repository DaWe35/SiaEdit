{
  "presets": [
    ["@babel/env", { "modules": false }]
  ],
  "plugins": ["@babel/transform-runtime", "@babel/plugin-transform-spread", "@babel/plugin-proposal-object-rest-spread"],
  "comments": false,
  "env": {
    "test": {
      "presets": ["env"],
      "plugins": ["transform-es2015-modules-commonjs", "dynamic-import-node", "@babel/plugin-transform-spread", "@babel/plugin-proposal-object-rest-spread"]
    }
  }
}
